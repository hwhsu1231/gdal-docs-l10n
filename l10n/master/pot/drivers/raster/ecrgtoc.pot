"  SUBDATASET_1_NAME=ECRG_TOC_ENTRY:ECRG:FalconView:1_500_K:ECRG_Sample/EPF/"
"TOC.xml\n"
"  SUBDATASET_1_DESC=Product ECRG, Disk FalconView, Scale 1:500 K"
