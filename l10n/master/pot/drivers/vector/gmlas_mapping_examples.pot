#: ../../../source/drivers/vector/gmlas_mapping_examples.rst:273
msgid "swe:DataArray"
msgstr ""

#: ../../../source/drivers/vector/gmlas_mapping_examples.rst:275
msgid "The following snippet"
msgstr ""

#: ../../../source/drivers/vector/gmlas_mapping_examples.rst:277
msgid ""
"<swe:DataArray>\n"
"    <swe:elementCount>\n"
"        <swe:Count>\n"
"                <swe:value>2</swe:value>\n"
"        </swe:Count>\n"
"    </swe:elementCount>\n"
"    <swe:elementType name=\"Components\">\n"
"        <swe:DataRecord>\n"
"                <swe:field name=\"myTime\">\n"
"                    <swe:Time definition=\"http://www.opengis.net/def/"
"property/OGC/0/SamplingTime\">\n"
"                            <swe:uom xlink:href=\"http://www.opengis.net/def/"
"uom/ISO-8601/0/Gregorian\"/>\n"
"                    </swe:Time>\n"
"                </swe:field>\n"
"                <swe:field name=\"myCategory\">\n"
"                    <swe:Category definition=\"http://dd.eionet.europa.eu/"
"vocabulary/aq/observationverification\"/>\n"
"                </swe:field>\n"
"                    <swe:field name=\"myQuantity\">\n"
"                    <swe:Quantity definition=\"http://dd.eionet.europa.eu/"
"vocabulary/aq/primaryObservation/hour\">\n"
"                            <swe:uom xlink:href=\"http://dd.eionet.europa.eu/"
"vocabulary/uom/concentration/ug.m-3\"/>\n"
"                    </swe:Quantity>\n"
"                </swe:field>\n"
"                <swe:field name=\"myCount\">\n"
"                    <swe:Count definition=\"http://\"/>\n"
"                </swe:field>\n"
"                    <swe:field name=\"myText\">\n"
"                    <swe:Text definition=\"http://\"/>\n"
"                </swe:field>\n"
"                    <swe:field name=\"myBoolean\">\n"
"                    <swe:Boolean definition=\"http://\"/>\n"
"                </swe:field>\n"
"        </swe:DataRecord>\n"
"    </swe:elementType>\n"
"    <swe:encoding>\n"
"            <swe:TextEncoding decimalSeparator=\".\" blockSeparator=\"@@\" "
"tokenSeparator=\",\"/>\n"
"    </swe:encoding>\n"
"    <swe:values>2016-09-01T00:00:00+01:00,1,2.34,3,foo,"
"true@@2017-09-01T00:00:00,2,3.45</swe:values>\n"
"</swe:DataArray>"
msgstr ""

