"    \"bounds\": \"2.000000,49.000000,3.000000,50.000000\",\n"
"    \"type\": \"overlay\",\n"
"    \"format\": \"pbf\",\n"
"    \"json\": \"{\n"
"        \\\"vector_layers\\\": [ {\n"
"            \\\"id\\\": \\\"my_layername\\\",\n"
"            \\\"description\\\": \\\"\\\",\n"
"            \\\"minzoom\\\": 0,\n"
"            \\\"maxzoom\\\": 0,\n"
"            \\\"fields\\\": {\n"
"                \\\"bool_false\\\": \\\"Boolean\\\",\n"
"                \\\"bool_true\\\": \\\"Boolean\\\",\n"
"                \\\"float_value\\\": \\\"Number\\\",\n"
"                \\\"pos_int_value\\\": \\\"Number\\\",\n"
"                \\\"pos_int64_value\\\": \\\"Number\\\",\n"
