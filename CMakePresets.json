{
  "version": 4,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 23,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "binaryDir": "${sourceDir}/out/build/${presetName}"
    },
    {
      "name": "all",
      "displayName": "All Languages",
      "description": "All Languages",
      "inherits": "default",
      "cacheVariables": {
        "LANGUAGE": "all"
      }
    },
    {
      "name": "zh_TW",
      "displayName": "Traditional Chinese",
      "description": "繁體中文",
      "inherits": "default",
      "cacheVariables": {
        "LANGUAGE": "zh_TW"
      }
    }
  ]
}